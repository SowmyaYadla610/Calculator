$ gcloud container clusters create $GKE_CLUSTER \
	--project=$DEMO_GKE_PROJECT \
	--zone=$WEST_US_ZONE
$ gcloud services enable \
	containerregistry.googleapis.com \
	container.googleapis.com
$ gcloud iam service-accounts create $Sowmya_NAME
$ gcloud iam service-accounts list
$ gcloud projects add-iam-policy-binding $GKE_PROJECT \
	--member=serviceAccount:$SA_sowmyachowdary610@gmail.com \
	--role=roles/container.admin
$ gcloud projects add-iam-policy-binding $GKE_PROJECT \
	--member=serviceAccount:$SA_sowmyachowdary610@gmail.com \
	--role=roles/storage.admin
$ gcloud projects add-iam-policy-binding $GKE_PROJECT \
	--member=serviceAccount:$SA_sowmyachowdary610@gmail.com \
	--role=roles/container.clusterViewer
$ gcloud iam service-accounts keys create key.json --iam-account=$SA_sowmyachowdary610@gmail.com

